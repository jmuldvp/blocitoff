<h1>User Information</h1>
<div class="row">
  <div class="col-md-4">
    <div class="media">
      <div class="media-body">
        User Name: <%= @user.email %><br />
        Sign in count: <%= @user.sign_in_count %><br />
        Account created: <%= @user.created_at %>
      </div>
    </div>
  </div>
</div>

<%= render @user.items %>

<%= render partial:'items/form', locals: { user: @user, item: Item.new } %>
